<h1>Sign Up</h1>

<%= form_for @user do |f| %>





    <%= f.label :username %><br>
  <%= f.text_field :username %>

    <%= f.label :email %><br />
    <%= f.email_field :email %>


    <%= f.label :password %><br />
    <%= f.password_field :password %>
  <%= f.submit "Signup" %>
<% end %>

<%= link_to "Your Shoe Entries", shoes_path %>
     <%= link_to "Your Handbag Entries", handbags_path %>
